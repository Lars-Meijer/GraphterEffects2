bounds(0, 500).

node(N) ->
    shape(N, ellipse),
    dimensions(N, 10, 10).

node(N1), node(N2) ->
    distance(N1, N2, ">", 10).

edge(N1, N2, E) ->
    line(N1, N2),
    distance(N1, N2, "<", 15),
//    shape(E, ellipse),
//    enclosed(N1, E),
