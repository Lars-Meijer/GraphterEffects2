node(X)                     -> text([X,0],"node").
label(X,"a")                -> text([X,1],"label").
flag(X,"b")                 -> text([X,2],"flag").
type(X,"c")                 -> text([X,3],"type").
attribute(X,"animal","cat") -> text([X,4],"attribute").
degree(X,1)                 -> text([X,5],"degree1").
degree(X,2)                 -> text([X,6],"degree2").
degree(X,3)                 -> text([X,7],"degree3").
degree(X,4)                 -> text([X,8],"degree4").
indegree(X,1)               -> text([X,9],"indegree1").
indegree(X,2)               -> text([X,10],"indegree2").
indegree(X,3)               -> text([X,11],"indegree3").
outdegree(X,0)              -> text([X,12],"outdegree0").
outdegree(X,1)              -> text([X,13],"outdegree1").
outdegree(X,2)              -> text([X,14],"outdegree2").
node(X), attributeCount(X,0)-> text([X,15],"attrcount0").
node(X), attributeCount(X,1)-> text([X,16],"attrcount1").
node(X), attributeCount(X,2)-> text([X,17],"attrcount2").
